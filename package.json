{
  "name": "documentor",
  "description": "Make documentation for DL projects",
  "version": "1.0.0",
  "main": "./source/build.js",
  "bin": {
    "documentor": "./build/build.js"
  },
  "scripts": {
    "init": "rimraf .validate.json && rimraf .jshintrc",
    "clean": "rimraf build",
    "lint": "eslint source test",
    "prebuild": "npm run clean",
    "build": "babel source --out-dir build && npm run build:doc",
    "build:doc": "doctoc --github --title \"## Contents\" ./docs && doctoc --github --title \"## Contents\" ./README.md",
    "test": "babel-node test/index.js",
    "cov": "npm run cov:clean && npm run cov:generate",
    "cov:clean": "rimraf coverage",
    "cov:generate": "babel-node node_modules/.bin/isparta cover --report text --report html test/index.js",
    "prepublish": "npm run build",
    "validate": "npm ls",
    "validate-dev": "npm run lint && npm run build && npm test | faucet",
    "audit": "nsp package",
    "deps": "npm run deps:missing && npm run deps:extra",
    "deps:missing": "dependency-check package.json",
    "deps:extra": "dependency-check package.json --extra --no-dev --ignore",
    "precheck": "npm run validate",
    "check": "npm run audit && npm run deps && npm outdated --depth 0"
  },
  "devDependencies": {
    "babel": "5.8.21",
    "babel-eslint": "4.0.5",
    "babel-loader": "5.3.2",
    "babel-plugin-object-assign": "1.2.1",
    "blue-tape": "0.1.10",
    "dependency-check": "2.5.0",
    "doctoc": "0.15.0",
    "eslint": "1.6.0",
    "eslint-config-airbnb": "0.1.0",
    "eslint-loader": "1.0.0",
    "eslint-plugin-react": "3.5.0",
    "faucet": "0.0.1",
    "isparta": "3.0.3",
    "node-libs-browser": "0.5.2",
    "nsp": "1.0.3",
    "rimraf": "2.4.2"
  },
  "author": "Matteo Ronchi <>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/cef62/documentor/issues"
  },
  "homepage": "https://github.com/cef62/documentor",
  "repository": {
    "type": "git",
    "url": "https://github.com/cef62/documentor.git"
  },
  "dependencies": {
    "handlebars": "4.0.3",
    "lodash.uniq": "3.2.2",
    "lunr": "0.5.12",
    "lunr-languages": "0.0.4",
    "markdown-it-checkbox": "1.1.0",
    "markdown-it-container": "1.0.0",
    "markdown-it-emoji": "1.1.0",
    "markdown-it-footnote": "1.0.0",
    "metalsmith": "2.1.0",
    "metalsmith-assets": "0.1.0",
    "metalsmith-babel": "3.0.0",
    "metalsmith-drafts": "0.0.1",
    "metalsmith-lunr": "0.2.1",
    "metalsmith-markdownit": "0.1.2",
    "metalsmith-navigation": "0.2.4",
    "metalsmith-templates": "0.7.0",
    "metalsmith-text-replace": "1.0.1",
    "minimatch": "3.0.0",
    "prismjs": "0.0.1",
    "slash": "1.0.0",
    "yargs": "3.26.0"
  }
}
